jpeg(file = "6-CD2GDT-doTPLOT_CASP_col.jpg", width = 10, height =3, units = "in", res = 500)
DotPlot(object = immune.combined, features = c("FAS", "ATM", "TP53", "BBC3", "BID", "BAK1", "BAX","PIDD1","CASP3"),cols = c("blue", "red"))
dev.off()


###########提取T细胞
rds<-subset(immune.combined,idents=c('CD2Ngd'))


################
DefaultAssay(rds) <- "RNA"

jpeg(file = "6-CD2GDT-doTPLOT_CASP_col.jpg", width = 10, height =3, units = "in", res = 500)
DotPlot(object = rds, features = c("CASP8","CASP3"), split.by = 'gruop',cols = c("blue", "red","purple"))
dev.off()


pdf(file="6-CD2GDT_CASP_voilin_spli.pdf",width=6,height=28)
plots <- VlnPlot(rds, features = c("FAS", "ATM", "TP53", "BBC3", "BID", "BAK1", "BAX","PIDD1","CASP3"), 
    pt.size = 0, split.by = 'gruop', combine = FALSE, log=TRUE)
wrap_plots(plots = plots, ncol = 1)
dev.off()
pdf(file="6-CD2GDT_CASP_voilin_spli.pdf",width=6,height=28)
plots <- VlnPlot(rds, features = c("ATM", "CASP8","CASP3"), 
    pt.size = 0, split.by = 'gruop', combine = FALSE, log=FALSE)
wrap_plots(plots = plots, ncol = 1)
dev.off()


jpeg(file = "6-CD2GDT-feature_CASP_col.jpg", width = 10, height =30, units = "in", res = 500)
FeaturePlot(rds, features = c("FAS", "ATM", "TP53", "BBC3", "BID", "BAK1", "BAX","PIDD1","CASP3"), min.cutoff = 0, max.cutoff = 3, cols = c("grey", "red"), reduction = "umap", split.by = 'gruop')
dev.off()


