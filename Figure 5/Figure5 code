# find markers for every cluster compared to all remaining cells, report only the positive

immune.combined.markers <- FindAllMarkers(immune.combined, only.pos = TRUE, min.pct = 0.25, logfc.threshold = 0.25)
immune.combined.markers %>%
    group_by(cluster) %>%
    top_n(n = 2, wt = avg_logFC)

write.csv(immune.combined.markers,file="CD2GDT_rename.markers.csv",row.names = F)


############
###########设置顺序#######################
immune.combined$gruop<-factor(immune.combined$gruop,levels=c('Human','Pig_con','Pig_t'))
###########设置顺序#######################
immune.combined.markers %>%
    group_by(cluster) %>%
    top_n(n = 10, wt = avg_logFC) -> top10

pdf(file="6-CD2GDT_markergene_allheatmap.pdf",width=7,height=7)
DoHeatmap(immune.combined, features = top10$gene) + scale_fill_gradientn(colors = c("blue", "white", "red")) + NoLegend()

dev.off()
#######################################3


jpeg(file = "6-CD2GDT-doTPLOT_top10_col.jpg", width = 10, height =3, units = "in", res = 500)
DotPlot(object = immune.combined, features = c("FOS", "JUN", "RHEX", "SELL", "STMN1", "CD2", "CD4","CD8A","TRDC"),cols = c("blue", "red"))
dev.off()

, split.by = 'gruop'
#######################################
save.image('pbmc-T-CD2gd-rename-201-new.Rdata')

